{
  "name": "@doderasoftware/restify-ai",
  "version": "0.2.0-beta.1",
  "description": "Professional AI Chatbot Vue 3 component for Laravel Restify backends. SSE streaming, file uploads, @mentions, context-aware suggestions, and full TypeScript support.",
  "type": "module",
  "main": "./dist/restify-ai.umd.cjs",
  "module": "./dist/restify-ai.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/restify-ai.js",
      "require": "./dist/restify-ai.umd.cjs",
      "types": "./dist/index.d.ts"
    },
    "./styles": "./dist/style.css",
    "./tailwind": "./tailwind.config.cjs"
  },
  "files": [
    "dist",
    "tailwind.config.cjs",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "dev": "vite --config vite.config.ts",
    "dev:playground": "vite --config playground/vite.config.ts",
    "build": "vue-tsc --noEmit && vite build",
    "build:only": "vite build",
    "build:watch": "vite build --watch",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx --fix",
    "type-check": "vue-tsc --noEmit",
    "prepublishOnly": "npm run build",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "version:beta": "npm version prerelease --preid=beta",
    "publish:beta": "npm publish --tag beta --access public",
    "publish:latest": "npm publish --access public",
    "format": "prettier --write 'src/**/*.{ts,vue}'",
    "lint:fix": "eslint . --ext .vue,.ts --fix"
  },
  "peerDependencies": {
    "pinia": ">=2.1.0",
    "tailwindcss": "^3.3.0",
    "vue": "^3.3.0"
  },
  "peerDependenciesMeta": {
    "tailwindcss": {
      "optional": true
    }
  },
  "dependencies": {
    "@microsoft/fetch-event-source": "^2.0.1",
    "dompurify": "^3.3.0",
    "marked": "^14.1.4"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.10",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^10.6.2",
    "globals": "^16.5.0",
    "pinia": "^2.1.7",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "~5.6.0",
    "typescript-eslint": "^8.50.1",
    "vite": "^5.4.11",
    "vite-plugin-dts": "^3.6.4",
    "vitest": "^1.6.1",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.12"
  },
  "keywords": [
    "vue",
    "vue3",
    "ai",
    "chatbot",
    "chat",
    "assistant",
    "ai-assistant",
    "laravel",
    "restify",
    "laravel-restify",
    "binarcode",
    "sse",
    "streaming",
    "component",
    "openai",
    "anthropic",
    "claude",
    "gpt",
    "llm",
    "tailwindcss",
    "typescript",
    "pinia",
    "vue-component",
    "chatgpt",
    "ai-chat"
  ],
  "author": {
    "name": "BinarCode",
    "email": "hello@binarcode.com",
    "url": "https://binarcode.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BinarCode/restify-ai.git"
  },
  "bugs": {
    "url": "https://github.com/BinarCode/restify-ai/issues"
  },
  "homepage": "https://laravel-restify.com",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/binarcode"
  }
}